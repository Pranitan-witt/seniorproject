  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
</head>

<body>
  <div id="mySidenav" class="sidenav">
    <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
    <center><img src="https://www.w3schools.com/images/w3schools_green.jpg" class="rounded-circle" alt="pic">
      <h2 style="margin-top: -20px;"><%- title%></h2>
      <hr style="background-color: whitesmoke;margin-top: -2px;">
      <h3 id="inbox">INBOX</h3>
      <hr style="background-color: whitesmoke;margin-top: -2px;">
      <a class="list" href="#">นายประณิธาน วิทยารณยุทธ</a>
