      <a class="list" href="#">นายธนกฤต บรรจงศิลป์</a>
    </center>
  </div>

  <div id="main" class="row">
    <span style="font-size:30px;cursor:pointer" onclick="openNav()">&#9776; VDEGREE</span>
    <div id="uuid">
      <input type="text" placeholder="123456789" disabled>
    </div>

    <div id="content" class="col-sm-12">
      <div id="content-box">
        <p>
          <!-- data -->
        </p>
      </div>
      <center>
        <button type="button" id="sentbtn" class="btn btn-dark btn-lg">อนุมัติ</button>
        <button type="button" id="sentbtn" class="btn btn-dark btn-lg">ไม่อนุมัติ</button>
      </center>
    </div>
  </div>

  <script>
    // Navbar
    function openNav() {
      document.getElementById("mySidenav").style.width = "250px";
      document.getElementById("main").style.marginLeft = "250px";
    }

    function closeNav() {
      document.getElementById("mySidenav").style.width = "0";
      document.getElementById("main").style.marginLeft = "0";
    }

  </script>

</body>

</html>